{
  "name": "@sklinet/strapi-plugin-preview-button",
  "description": "Add a preview button and live view button to the content manager edit view to connect with your frontend app.",
  "version": "1.0.2",
  "keywords": [],
  "author": {
    "name": "SKLINET s.r.o.",
    "url": "https://github.com/SKLINET"
  },
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "./dist/admin/src/index.d.ts",
      "source": "./admin/src/index.ts",
      "import": "./dist/admin/index.mjs",
      "require": "./dist/admin/index.js",
      "default": "./dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.12",
    "@strapi/icons": "^2.0.0-rc.12",
    "lodash": "^4.17.21",
    "react-intl": "^6.8.4"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "@strapi/typescript-utils": "^5.2.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.2.7",
    "@strapi/strapi": "^5.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0",
    "styled-components": "^6.1.13"
  },
  "strapi": {
    "kind": "plugin",
    "name": "preview-button",
    "displayName": "Preview button",
    "description": "Add a preview button and live view button to the content manager edit view to connect with your frontend app."
  },
  "license": "MIT"
}
